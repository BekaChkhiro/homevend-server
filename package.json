{
  "name": "homevend-backend",
  "version": "1.0.0",
  "description": "Backend API for Homevend real estate platform",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "build": "npm install --production=false && tsc",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:run:prod": "node -r ./dist/data-source.js node_modules/typeorm/cli.js migration:run",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "migration:generate": "tsx node_modules/typeorm/cli.js migration:generate -d ormconfig.ts",
    "migration:run": "tsx node_modules/typeorm/cli.js migration:run -d ormconfig.ts",
    "migration:revert": "tsx node_modules/typeorm/cli.js migration:revert -d ormconfig.ts"
  },
  "keywords": [
    "real-estate",
    "property",
    "api",
    "backend"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/helmet": "^0.0.48",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.2.0",
    "@types/pg": "^8.15.5",
    "bcrypt": "^6.0.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.25",
    "typescript": "^5.9.2",
    "zod": "^4.0.15"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^9.32.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3"
  }
}
